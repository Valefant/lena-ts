    "demo": "tsdx build && http-server",
    "pub": "npm publish",
    "prepare": "tsdx build"
  },
  "peerDependencies": {},
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "http-server": "^0.12.3",
    "husky": "^5.1.3",
